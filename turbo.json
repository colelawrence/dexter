{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "outputs": ["dist/**"],
      "dependsOn": ["^build"]
    },
    "test": {
      "dependsOn": [],
      "inputs": ["**/*.test.ts", "**/*.test.tsx"]
    },
    "lint": {
      "dependsOn": [],
      "outputs": ["node_modules/.cache/eslint/**"]
    },
    "typecheck": {
      "dependsOn": []
    },
    "dev": {
      "cache": false
    },
    "clean": {
      "cache": false
    }
  }
}
